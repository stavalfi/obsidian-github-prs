{
	"name": "obsidian-github-prs",
	"version": "1.1.0",
	"description": "The plugin prints PRs in a table in Obsidian",
	"main": "main.js",
	"repository": {
		"url": "https://github.com/stavalfi/obsidian-github-prs"
	},
	"scripts": {
		"github:relase": "yarn build && gh release create 1.1.0 main.js manifest.json styles.css",
		"lint-format": "biome check --apply-unsafe --error-on-warnings",
		"lint-format:all": "npm run lint-format .",
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [],
	"author": "Stav Alfi",
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "1.2.2",
		"@types/humanize-duration": "^3.27.2",
		"@types/lodash": "^4.14.200",
		"@types/node": "^16.11.6",
		"@types/react": "^18.2.29",
		"@types/react-dom": "^18.2.14",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"humanize-duration": "^3.30.0",
		"lodash": "^4.17.21",
		"octokit": "^3.1.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	}
}
